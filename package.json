{
  "name": "assessment-helper",
  "version": "0.1.0",
  "private": true,
  "main": "server/server",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "debounce": "^1.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "json-server": "^0.12.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.0",
    "nodemon": "^1.17.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-notify-toast": "^0.4.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"json-server --watch db.json --port 3001\" \"nodemon\"",
    "build": "react-scripts build",
    "dev": "json-server --watch db.json --port 3001 | nodemon server/server.js | react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3333"
    }
  }
}
